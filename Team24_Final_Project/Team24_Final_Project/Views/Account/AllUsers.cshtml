@model Team24_Final_Project.Models.AppUser

@{
    ViewData["Title"] = "AllUsers";
}

<h1>Get All Users</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="EditUsers">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label class="contorl-label">Users:</label>
                @Html.DropDownList("SelectedUserID", (SelectList)ViewBag.AllUsers, new { @class = "form-control" })
            </div>
            <div class="form-group">
                <input type="submit" value="Select User" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>